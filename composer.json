{
    "name": "moom/db",
    "description": "DB package.",
    "keywords": [
        "MOOM",
        "MOOM-DB"
    ],
    "homepage": "https://github.com/moustafaomar/DB",
    "license": "The Unlicense",
    "type": "library",
    "authors": [
        {
            "name": "Mostafa Omar",
            "email": "moustafaomar200@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MOOM\\DB\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MOOM\\DB\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
